<% content_for :gobierto_head_content do %>

  <meta name="page_title" content="<%= raw (@meta_tags ? @meta_tags['title'] : nil) %>">

  <%= javascript_tag do %>
    commonControllerLoadArgs = {
      people_events_first_date: '<%= all_start_date.try(:iso8601) %>',
      people_events_filter_start_date: '<%= filter_start_date.try(:iso8601) %>',
      people_events_filter_end_date: '<%= filter_end_date.try(:iso8601) %>'
    }

  <% end %>
<% end %>

<% content_for :javascript_module_link do %>
  <%= javascript_packs_with_chunks_tag 'people', 'gencat', 'data-turbolinks-track': true %>
<% end %>

<% content_for(:current_submodule_link) do %>
  <% if controller_name == "welcome" %>
    <%= link_to t("layouts.application.start"), gobierto_people_root_path %>
  <% end %>
<% end %>

<% content_for :gobierto_footer_content do %>
  <%= render "gobierto_people/shared/breadcrumb" %>
<% end %>

<%= render template: "layouts/application" %>
